{{#each product-categories}}
{{#each packagings}}
{{#each quantities}}
<div class="card">
	<img class="card-img-top" src="./Assets/milk.jpg" alt="Card image cap">
	<div class="card-body">
		<h5 class="card-title">{{../brand}}</h5>
		<span class="price">
			<i class="fa fa-inr" aria-hidden="true"></i>
			<span class="card-text"></span>{{this}}</span>
		</div>
		<div class="card-footer">
			<div class="row">
				<div class="col-sm-6 text-center card-quant-col">
					<span>
						<i class="fa fa-plus" aria-hidden="true"></i>
					</span>
					<span class="card-quantity">12</span>
					<span>
						<i class="fa fa-minus" aria-hidden="true"></i>
					</span>
				</div>
				<div class="col-sm-6 text-center card-cart">
					<i class="fa fa-cart-plus fa-lg" aria-hidden="true"></i>
					<span>Add</span>
				</div>
			</div>
		</div>
	</div>
{{/each}}
{{/each}}
{{/each}}